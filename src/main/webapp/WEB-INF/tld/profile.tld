<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
        version="2.0">

    <tlib-version>2.0</tlib-version>
    <short-name>profile</short-name>

    <function>
        <description>获取性别字典</description>
        <name>getGenderMap</name>
        <function-class>edu.gdei.gdeiassistant.Service.Profile.UserProfileService</function-class>
        <function-signature>java.util.Map getGenderMap()</function-signature>
        <example>${profile:getGenderMap()}</example>
    </function>

    <function>
        <description>获取职业字典</description>
        <name>getProfessionMap</name>
        <function-class>edu.gdei.gdeiassistant.Service.Profile.UserProfileService</function-class>
        <function-signature>java.util.Map getProfessionMap()</function-signature>
        <example>${profile:getProfessionMap()}</example>
    </function>

    <function>
        <description>获取学历字典</description>
        <name>getDegreeMap</name>
        <function-class>edu.gdei.gdeiassistant.Service.Profile.UserProfileService</function-class>
        <function-signature>java.util.Map getDegreeMap()</function-signature>
        <example>${profile:getDegreeMap()}</example>
    </function>

    <function>
        <description>获取院系字典</description>
        <name>getFacultyMap</name>
        <function-class>edu.gdei.gdeiassistant.Service.Profile.UserProfileService</function-class>
        <function-signature>java.util.Map getFacultyMap()</function-signature>
        <example>${profile:getFacultyMap()}</example>
    </function>

</taglib>